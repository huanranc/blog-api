{
  "compilerOptions": {
      "lib": ["es5", "es6"],
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "outDir": "dist",  // TS文件编译后会放入到此文件夹内
  },
  "exclude": [
      "node_modules"
  ]
}
